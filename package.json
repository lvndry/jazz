  "scripts": {
    "build": "bun build src/main.ts --outfile dist/main.js --target node --packages external && tsc --emitDeclarationOnly",
    "typecheck": "bun run tsc --noEmit",
    "dev": "bun --watch src/main.ts",
    "start": "bun dist/main.js",
    "cli": "bun src/main.ts",
    "lint": "bun eslint src/**/*.ts",
    "lint:fix": "bun eslint src/**/*.ts --fix",
    "format": "bun prettier --write src/**/*.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:typecheck": "bunx tsc --project tsconfig.test.json",
    "clean": "rm -rf dist",
    "prebuild": "bun run clean",
    "prepublishOnly": "bun run build"
